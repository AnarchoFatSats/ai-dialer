# Environment
ENVIRONMENT=development
DEBUG=true

# Database - AWS RDS PostgreSQL
DATABASE_URL=postgresql://postgres:password@your-rds-endpoint.us-east-1.rds.amazonaws.com:5432/aidialer
REDIS_URL=redis://your-elasticache-endpoint.cache.amazonaws.com:6379

# AWS RDS Configuration
RDS_ENDPOINT=your-rds-endpoint.us-east-1.rds.amazonaws.com
RDS_PORT=5432
RDS_DATABASE=aidialer
RDS_USERNAME=postgres
RDS_PASSWORD=your_rds_password

# AWS ElastiCache Configuration
ELASTICACHE_ENDPOINT=your-elasticache-endpoint.cache.amazonaws.com
ELASTICACHE_PORT=6379

# AWS Configuration (required)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
S3_BUCKET_NAME=your-s3-bucket

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_account_sid
TWILIO_AUTH_TOKEN=your_auth_token
TWILIO_PHONE_NUMBER=+1234567890
WEBHOOK_BASE_URL=https://your-domain.com

# System Configuration
BASE_URL=https://your-domain.com
DOMAIN=your-domain.com
SPAM_CHECK_API_KEY=your_spam_check_api_key

# Voice & Audio Settings
AUDIO_SAMPLE_RATE=8000
AUDIO_FORMAT=mulaw
MAX_AUDIO_CHUNK_SIZE=8000
SPEECH_DETECTION_THRESHOLD=0.7

# AI Services
ANTHROPIC_API_KEY=your_anthropic_key
DEEPGRAM_API_KEY=your_deepgram_key
ELEVENLABS_API_KEY=your_elevenlabs_key
ELEVENLABS_VOICE_ID=pNInz6obpgDQGcFmaJgB  # Default voice ID (Adam)
OPENAI_API_KEY=your_openai_key

# AI Configuration
CLAUDE_MODEL=claude-3-haiku-20240307
DEEPGRAM_MODEL=nova-2
ELEVENLABS_MODEL=eleven_turbo_v2
MAX_AI_RESPONSE_TIME_MS=800
AI_CONVERSATION_TIMEOUT_SECONDS=300

# Reputation & Compliance
NUMERACLE_API_KEY=your_numeracle_key
FREE_CALLER_REGISTRY_API_KEY=your_fcr_key
DNC_REGISTRY_USERNAME=your_dnc_username
DNC_REGISTRY_PASSWORD=your_dnc_password

# Cost Optimization
MAX_COST_PER_MINUTE=0.025
MAX_DAILY_BUDGET=1000.00
COST_ALERT_THRESHOLD=0.80

# Campaign Settings
MAX_CONCURRENT_CALLS=1000
DEFAULT_RETRY_ATTEMPTS=3
CALL_TIMEOUT_SECONDS=30
MAX_CALLS_PER_DID_DAILY=150
MAX_TALK_TIME_PER_DID_DAILY=1200

# Analytics & Monitoring
PROMETHEUS_GATEWAY_URL=http://localhost:9091
GRAFANA_URL=http://localhost:3000
SENTRY_DSN=your_sentry_dsn

# Security
JWT_SECRET_KEY=your_jwt_secret_key
WEBHOOK_VERIFICATION_TOKEN=your_webhook_token
ENCRYPTION_KEY=your_encryption_key

# AWS Connect Configuration
AWS_CONNECT_INSTANCE_ID=your_connect_instance_id
AWS_CONNECT_INSTANCE_ARN=your_connect_instance_arn
AWS_CONNECT_CONTACT_FLOW_ID=your_contact_flow_id
AWS_CONNECT_QUEUE_ID=your_queue_id

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json 